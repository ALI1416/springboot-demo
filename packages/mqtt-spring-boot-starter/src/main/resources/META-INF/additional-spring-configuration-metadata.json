      "sourceType": "cn.z.mqtt.autoconfigure.MqttSslProperties",
      "defaultValue": false
    },
    {
      "name": "mqtt.ssl.ckeck",
      "type": "java.lang.Boolean",
      "description": "校验SSL证书.",
      "sourceType": "cn.z.mqtt.autoconfigure.MqttSslProperties",
      "defaultValue": false
    },
    {
      "name": "mqtt.ssl.ca-server-auto-sign",
      "type": "java.lang.Boolean",
