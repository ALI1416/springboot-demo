    },
    {
      "name": "mqtt.ssl.client-key-local-path",
      "type": "java.lang.String",
      "description": "客户端密钥(本地路径)(优先级1),读取本地物理路径.",
      "sourceType": "cn.z.mqtt.autoconfigure.MqttSslProperties"
