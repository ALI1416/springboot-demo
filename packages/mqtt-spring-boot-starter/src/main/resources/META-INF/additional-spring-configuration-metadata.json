{
  "properties": [
    {
      "name": "mqtt.uri",
      "type": "java.lang.String",
      "description": "URI(ssl例如\"ssl://127.0.0.1:8883\").",
      "sourceType": "cn.z.mqtt.autoconfigure.MqttProperties",
      "defaultValue": "tcp://127.0.0.1:1883"
    },
    {
      "name": "mqtt.username",
      "type": "java.lang.String",
      "description": "用户名.",
      "sourceType": "cn.z.mqtt.autoconfigure.MqttProperties"
    },
    {
      "name": "mqtt.password",
      "type": "java.lang.String",
      "description": "密码.",
      "sourceType": "cn.z.mqtt.autoconfigure.MqttProperties"
    },
    {
      "name": "mqtt.connection-timeout",
      "type": "java.lang.Integer",
